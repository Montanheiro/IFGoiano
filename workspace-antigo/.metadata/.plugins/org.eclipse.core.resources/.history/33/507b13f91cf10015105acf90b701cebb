		System.out.println("Funcionáŕio: " + FolhaPagamento.nomeFunc + "\nFunção: " + FolhaPagamento.Funcao + "\nSalário atual: " + FolhaPagamento.salFunc + "\nNovo Salário: " + FolhaPagamento.salarioNovo);
		
		
	}

}
